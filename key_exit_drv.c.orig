    }
		    if(GPIO_Pin==S1_Pin)
    {
        HAL_GPIO_TogglePin(LED1_GPIO_Port,LED1_Pin);//反转LED IO
        HAL_GPIO_TogglePin(BEEP_GPIO_Port,BEEP_Pin);//反转蜂鸣器IO
    }
		    if(GPIO_Pin==S1_Pin)
    {
        HAL_GPIO_TogglePin(LED1_GPIO_Port,LED1_Pin);//反转LED IO
        HAL_GPIO_TogglePin(BEEP_GPIO_Port,BEEP_Pin);//反转蜂鸣器IO
    }
}
